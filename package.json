{
  "name": "@jnv/lists-manage",
  "version": "0.5.3",
  "main": "lib/index.js",
  "license": "MIT",
  "type": "module",
  "engines": {
    "//": "needed for named capture groups and fs.promise",
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": "github:jnv/lists-manage",
  "bin": {
    "lists-manage": "./bin/run"
  },
  "homepage": "https://github.com/jnv/lists-manage",
  "bugs": "https://github.com/jnv/lists-manage/issues",
  "author": "Jan Vlnas <npm@jan.vlnas.cz> (https://jan.vlnas.cz/)",
  "dependencies": {
    "@oclif/command": "^1.8.36",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-help": "^6",
    "debug": "^4.3.6",
    "enquirer": "^2.4.1",
    "got": "^14.4.7",
    "hosted-git-info": "^8.0.2",
    "simple-git": "^3.11.0",
    "tslib": "^2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@oclif/dev-cli": "^1.26.10",
    "@types/debug": "^4.1.12",
    "@types/hosted-git-info": "^3.0.5",
    "@types/node": "^24.0.13",
    "nock": "^13.5.6",
    "ts-node": "^10",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "test": "node --experimental-strip-types --test './src/**/*.test.ts'",
    "version": "oclif-dev readme && git add README.md",
    "prebuild": "rm -rf lib",
    "build": "tsc -b tsconfig.build.json",
    "prepack": "npm run build && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "lists-manage",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "types": "lib/index.d.ts"
}
