{
  "name": "@jnv/lists-manage",
  "version": "0.5.3",
  "main": "lib/index.js",
  "license": "MIT",
  "engines": {
    "//": "needed for named capture groups and fs.promise",
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": "github:jnv/lists-manage",
  "bin": {
    "lists-manage": "./bin/run"
  },
  "homepage": "https://github.com/jnv/lists-manage",
  "bugs": "https://github.com/jnv/lists-manage/issues",
  "author": "Jan Vlnas <npm@jan.vlnas.cz> (https://jan.vlnas.cz/)",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-help": "^3",
    "debug": "^4.3.2",
    "enquirer": "^2.3.6",
    "gh-got": "^9.0.0",
    "got": "^11.8",
    "hosted-git-info": "^4.0.2",
    "simple-git": "^2.45.0",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.0",
    "nock": "^13.1.3",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint --ext .js,.ts src/",
    "version": "oclif-dev readme && git add README.md",
    "prebuild": "rm -rf lib",
    "build": "tsc -b tsconfig.build.json",
    "prepack": "npm run build && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "lists-manage",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "types": "lib/index.d.ts"
}
